/*******************************
    Galette Variable Overrides
*******************************/

@styledBoxShadow : 0 1px 2px 0 @primaryColor, 0 0 0 1px @primaryColor;
@styledTitleBackground: @textColor;
@styledTitleColor: @white;
@styledActiveTitleBackground: @lightPrimaryColor;
@styledActiveTitleColor: darken(@textColor, 15);
@styledTitleHoverBackground: @primaryColor;
@styledTitleHoverColor: darken(@textColor, 15);
